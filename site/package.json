{
  "name": "renderless-svelte-site",
  "description": "Website for renderless svelte",
  "version": "1.0.0",
  "scripts": {
    "build": "sapper build --legacy --ext \".svelte .svx\"",
    "dev": "sapper dev --ext \".svelte .svx\"",
    "export": "sapper export --legacy --ext \".svelte .svx\" --entry \"/ /404\"",
    "start": "node __sapper__/build",
    "serve": "sirv __sapper__/export"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0",
    "sirv-cli": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "renderless-svelte": "^1.5.0",
    "mdsvex": "^0.8.9",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3"
  }
}
